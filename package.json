{
  "name": "nuxt-pluto",
  "version": "1.0.0",
  "description": "My extraordinary Nuxt.js project",
  "author": "ka2jun8",
  "private": true,
  "scripts": {
    "dev": "run-p dev:{agreed,server}",
    "dev:agreed": "agreed-server --path ./spec/agreed/agreed.js --port 3010",
    "dev:server": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev:generate": "run-p dev:agreed generate",
    "start": "run-s dev",
    "build": "nuxt build",
    "start:prod": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest --config jest.config.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "date-fns": "^1.30.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "multer": "^1.4.1",
    "nuxt": "^2.8.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.28",
    "agreed": "^3.2.2",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.6.0",
    "babel-preset-vue-app": "^2.0.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^23.6.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.1.5",
    "nuxt-sass-resources-loader": "^2.0.5",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.2"
  }
}
